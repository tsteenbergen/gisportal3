{
    "kind": "Service",
    "apiVersion": "v1",
    "metadata": {
        "labels": {
			"name": "$name",
			"namespace": "$namespace"
		}
    },
    "spec": {
        "sessionAffinity": "None",
        "type": "ClusterIP",
        "ports": [
            {
                "name": "$name",
                "port": 80,
                "protocol": "TCP",
                "targetPort": 80
            }
        ]
    },
    "status": {
        "loadBalancer": {}
    }
}