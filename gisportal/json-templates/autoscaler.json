{
    "kind": "HorizontalPodAutoscaler",
    "apiVersion": "autoscaling/v1",
    "metadata": {
        "name": "$name",
        "namespace": "$namespace",
        "labels": {
			"name": "$name"
        }
    },
    "spec": {
		"minReplicas": "1",
		"maxReplicas": "5",
        "targetCPUUtilizationPercentage": "80%"
    },
	"scaleTargetRef": {
		"apiVersion": "v1",
		"kind": "DeploymentConfig",
		"name": "$name"
	}
}